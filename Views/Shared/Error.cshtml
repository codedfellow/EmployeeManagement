@if (ViewBag.ErrorTitle == null)
{
    <h3>An error occured while processing your request. The support team is notified and we are working on it</h3>
    <h5>Please contact us at fitraksoftware.com</h5>
    <hr />
}
else
{
    <h1 class="text-danger text-center">@ViewBag.ErrorTitle</h1>
    <h6 class="text-danger">@ViewBag.ErrorMessage</h6>
}
